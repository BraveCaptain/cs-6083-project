      <div id="content">

        <!-- Topbar -->
        {{include '../common/supertopbar.art'}}
        <!-- End of Topbar -->
        <div class="container-fluid">

          <!-- Page Heading -->
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">My Profile</h1>
          </div>

         <div class="row">

            <!-- Earnings (Monthly) Card Example -->
