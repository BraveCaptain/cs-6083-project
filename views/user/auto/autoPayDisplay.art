<!DOCTYPE html>
<html lang="en">

<head>

  {{include '../../common/header.art'}}
  <title>WDS {{userid}} Auto Payment Display</title>

  <!-- Custom scripts for all pages-->
  <script src="/js/sb-admin-2.min.js"></script>

  <!-- Page level plugins -->
  <script src="/vendor/chart.js/Chart.min.js"></script>


  <meta name="author" content="">

  <!--
  <script>
    function registerHome() {
      var res = formContentToJson($("#registerForm"));
      //if user did not enter userid or password
      return validateHomeInput(res);
    }
  </script>
  -->

</head>

<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <!-- Sidebar -->
    {{include '../../common/sidebar.art'}}
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <!-- Topbar -->
        {{include '../../common/topbar.art'}}
        <!-- End of Topbar -->

        <!-- Begin Page Content -->

        <!-- Page Heading -->          

        <div class="container-fluid">
        <table class="table table-striped table-bordered table-hover custom-table">
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-2 text-gray-800">Auto Payment Information of {{userid}}</h1>
          </div>
          <!-- DataTales Example -->

          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">



          <thead>
            <tr>
              <th>Auto Payment ID</th>
              <th>Payment Date</th>
              <th>Payment Method</th>
              <th>Auto Invoice ID Paid for</th>
              <th>Payment Amount</th>
            </tr>
          </thead>
          <tfoot>
            <tr>
              <th>Auto Payment ID</th>
              <th>Payment Date</th>
              <th>Payment Method</th>
              <th>Auto Invoice ID Paid for</th>
              <th>Payment Amount</th>
            </tr>
          </tfoot>
          <tbody>
            {{each autoPayInfo}}

            <tr>
              <td>{{$value.paymentid}}</td>
              <td>{{$value.paymentdate}}</td>
              <td>{{$value.method}}</td>
              <td>{{$value.apid}}</td>
              <td>${{$value.amount}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
        </div>
        </div>


          



        

        <!-- /.container-fluid -->
      </div>
      </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      {{include '../../common/footer.art'}}
      <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>


</body>